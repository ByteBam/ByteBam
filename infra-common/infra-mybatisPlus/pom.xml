        <artifactId>infra-commonHook</artifactId>
        <version>1.0.0</version>
    </parent>

    <packaging>jar</packaging>

    <artifactId>infra-mybatisPlus</artifactId>
    <version>1.0.0</version>
    <name>infra-mybatisPlus</name>
